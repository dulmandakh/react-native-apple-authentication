{
  "name": "react-native-apple-authentication-example",
  "version": "6.1.0",
  "private": true,
  "scripts": {
    "build:clean": "rimraf ios/build",
    "prepare": "patch-package",
    "start": "yarn react-native start --reset-cache",
    "run:android": "yarn react-native run-android",
